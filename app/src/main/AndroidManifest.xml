<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lisowski.wojtek.wkuwaczswek">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseLevel"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EditWord"
            android:label="@string/editWord"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddWords"
            android:label="@string/addWords"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DifficultTest"
            android:label="@string/difficultTest"
            android:parentActivityName=".ChooseLevel" />
        <activity
            android:name=".EasyTest"
            android:label="@string/easyTest"
            android:parentActivityName=".ChooseLevel" />
        <activity
            android:name=".SectionListView"
            android:parentActivityName=".ChooseLevel" />
        <activity android:name=".SelectSection" />
        <activity android:name=".SelectWord" />
        <activity android:name=".SelectSectionInAddWords" />
        <activity
            android:name=".WordsPreview"
            android:label="@string/wordsPreview"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".EditSection"
            android:label="@string/editSection"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>