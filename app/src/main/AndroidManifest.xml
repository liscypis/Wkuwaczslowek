<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lisowski.wojtek.wkuwaczswek">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ChooseLevel"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.EditWord"
            android:label="@string/editWord"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.AddWords"
            android:label="@string/addWords"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.DifficultTest"
            android:label="@string/difficultTest"
            android:parentActivityName=".activities.ChooseLevel" />
        <activity
            android:name=".activities.EasyTest"
            android:label="@string/easyTest"
            android:parentActivityName=".activities.ChooseLevel" />
        <activity
            android:name=".activities.WordsPreview"
            android:label="@string/wordsPreview"
            android:parentActivityName=".activities.MainActivity"/>
        <activity
            android:name=".activities.EditSection"
            android:label="@string/editSection"
            android:parentActivityName=".activities.MainActivity"/>
    </application>

</manifest>